source etc/banner
source etc/PIPELINE


cd $down
if [[ -d "output" ]]
  then
    echo ""
  else
    echo "making folder output"
    mkdir $down/output
fi


cd $down
if [[ -d "dmp" ]]
  then
    echo ""
  else
    echo "making file dump"
    mkdir $down/dmp
fi
cd $tx/etc
if [[ -d "folderuser" ]]
  then
    echo "DOWNLOADING"
  else
    echo "making file folderuser"
    mkdir $tx/etc/folderuser

echo "PROCCESING FILE"
sleep 1
bash enc
